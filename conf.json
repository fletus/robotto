{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        include: ['./lib'],
        exclude: [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "/docs/templates/jaguar",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "tutorials": "docs/tutorials",
    }
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
